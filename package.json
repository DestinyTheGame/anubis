{
  "name": "anubis",
  "version": "0.0.8",
  "description": "A lightweight trials companion for destiny the game.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=bungie-auth* electron .",
    "webpack": "node -r babel-register webpack.js",
    "dev": "concurrently -k 'npm run webpack' 'npm start'",
    "prod": "NODE_ENV=production webpack --config webpack.prod.babel.js",
    "manifest": "node ./tools/build.js",
    "release": "npm run prod && electron-packager . Anubis --all --prune --overwrite --ignore builds --out builds"
  },
  "keywords": [
    "trials",
    "companion",
    "app",
    "anubis"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "dependencies": {
    "async": "2.1.x",
    "babel-ignore": "2.0.x",
    "babel-loader": "6.3.x",
    "babel-plugin-transform-object-rest-spread": "6.22.x",
    "babel-preset-es2015": "6.22.x",
    "babel-preset-react": "6.22.x",
    "babel-register": "6.22.x",
    "booting": "1.0.x",
    "bungie-auth": "0.0.x",
    "connected": "0.0.x",
    "destiny-api": "1.2.x",
    "electron-json-storage": "3.0.x",
    "electron-prebuilt": "1.4.x",
    "express": "4.14.x",
    "git-shizzle": "0.0.x",
    "guardian.gg": "0.0.x",
    "halogen": "0.2.x",
    "intl-messageformat": "1.3.x",
    "portfinder": "1.0.x",
    "react": "15.4.x",
    "react-codemirror": "0.3.x",
    "react-dom": "15.4.x",
    "react-router": "3.0.x",
    "react-toggle": "~3.0.0",
    "react-tooltip": "3.2.x",
    "stale-state": "1.1.x",
    "tick-tock": "1.0.x",
    "tmi.js": "1.1.x",
    "ws": "1.1.x",
    "xmlhttprequest": "1.8.x",
    "yeast": "0.1.x"
  },
  "devDependencies": {
    "asar": "0.13.x",
    "concurrently": "3.2.x",
    "electron": "1.4.x",
    "electron-devtools-installer": "2.1.x",
    "electron-packager": "8.5.x",
    "git-rev-sync": "1.8.x",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.13.2"
  },
  "babel-ignore": false,
  "babel": {
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  }
}
