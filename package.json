{
  "name": "anubis",
  "version": "0.0.0",
  "description": "A lightweight trials companion for destiny the game.",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'node -r babel-register webpack.js' 'npm start'",
    "build": "NODE_ENV=production webpack --config webpack.prod.babel.js && electron-packager . Anubis --platform=darwin --arch=all --prune --overwrite"
  },
  "keywords": [
    "trials",
    "companion",
    "app",
    "anubis"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "dependencies": {
    "async": "2.1.4",
    "babel-loader": "6.2.8",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "booting": "1.0.0",
    "bungie-auth": "0.0.1",
    "connected": "0.0.2",
    "electron-json-storage": "2.1.0",
    "electron-packager": "8.3.0",
    "electron-prebuilt": "1.4.10",
    "intl-messageformat": "1.3.0",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "svgs": "1.0.0",
    "tmi.js": "1.1.2",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "electron": "1.4.10",
    "electron-devtools-installer": "2.0.1",
    "electron-packager": "8.3.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.13.2",
    "webpack": "^2.1.0-beta.25",
    "express": "^4.14.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "babel": {
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "presets": [
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  }
}
